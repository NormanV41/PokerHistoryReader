{
  "name": "pokertracker",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "inspect": "tsc -p . && node --inspect-brk lib/index.js && rm -r lib/",
    "format": "prettier --write \"**/*.ts\" \"**/*.html\" \"**/*.scss\" \"**/*.json !(data/)\"",
    "lint": "tslint \"src/**/*.ts\""
  },
  "prettier": {
    "arrowParens": "always"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/moment-timezone": "^0.5.12",
    "@types/mysql": "^2.15.7",
    "@types/node": "^12.7.12",
    "@types/rimraf": "^2.0.2",
    "node-key-sender": "^1.0.11",
    "nodemon": "^1.19.3",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "mysql": "^2.17.1",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3"
  }
